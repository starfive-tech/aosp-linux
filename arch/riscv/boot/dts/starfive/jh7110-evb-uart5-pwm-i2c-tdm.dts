/dts-v1/;
#include "jh7110-evb.dtsi"
#include "codecs/sf_tdm.dtsi"

/ {
	model = "StarFive JH7110 EVB";
	compatible = "starfive,jh7110-evb", "starfive,jh7110";
};

&uart5 {
	status = "okay";
};

&ptc {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm_ch0to3_pins &pwm_ch4to5_pins>;
	status = "okay";
};

&tdm {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c3 {
	status = "okay";
};
